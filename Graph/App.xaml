<Application x:Class="Graph.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Graph"
             xmlns:graphlib="clr-namespace:GraphLib;assembly=GraphLib"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <graphlib:IsPositiveIntConverter x:Key="intConverter"></graphlib:IsPositiveIntConverter>
        <graphlib:MyMultipleConverter x:Key="multiConverter"></graphlib:MyMultipleConverter>
        <graphlib:HasTextConverter x:Key="hasTextConverter"></graphlib:HasTextConverter>
        <graphlib:IsPositiveDoubleConverter x:Key="doubleConverter"></graphlib:IsPositiveDoubleConverter>
    </Application.Resources>
</Application>
